@model IEnumerable<MMS.web.Models.imagesviewmodel>
@{
    Layout = "~/Views/Shared/_LayoutCKEDIT.cshtml";
}
@foreach (var image in Model)
{
    <a href="#" class="returnImage" data-url="@image.Url">
        <img src="@image.Url" alt="Hejsan" id="#image" data-source="@image.Url" width="200" height="200" />
    </a>
}
<script type="text/javascript">
    $(document).ready(function () {
        $(".returnImage").click("click", function (e) {
            var urlImage = $(this).attr("data-url");
            window.opener.updateValue(urlImage);
            window.close();
        });
    });
</script>
